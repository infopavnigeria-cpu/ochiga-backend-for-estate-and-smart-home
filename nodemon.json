{
  "watch": ["src"],
  "ext": "ts,js,json",
  "ignore": [
    "node_modules",
    "dist",
    "data",
    "certs",
    ".git",
    ".env",
    "package.json",
    "package-lock.json"
  ],
  "exec": "ts-node -r tsconfig-paths/register src/main.ts",
  "delay": "500",
  "env": {
    "NODE_ENV": "development"
  }
}
